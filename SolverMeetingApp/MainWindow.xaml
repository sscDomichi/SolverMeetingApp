            <Button x:Name="DummyReadButton" Content="DummyRead" HorizontalAlignment="Left" Margin="0,214,0,0" VerticalAlignment="Top" Width="100" Click="DummyReadButton_Click"/>
        </Grid>
        <Grid HorizontalAlignment="Left" Height="60" Margin="10,249,0,0" VerticalAlignment="Top" Width="497"/>

    </Grid>
</Window>
