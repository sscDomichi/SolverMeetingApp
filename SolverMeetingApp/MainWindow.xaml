        <Grid HorizontalAlignment="Left" Height="234" Margin="10,10,0,0" VerticalAlignment="Top" Width="480">
            <StackPanel x:Name="AttendanceStackPanel">
                <local:AttendanceControl></local:AttendanceControl>
